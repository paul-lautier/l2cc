{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/l2cc/browser"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build/index.html"
      }
    ],
    "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]  }
  